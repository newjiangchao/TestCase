<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="SmokeTestSuite" parallel="classes" thread-count="1">

    <test verbose="1" name="AirwallexSmokeTests">
        <groups>
            <run>
                <include name="smoke" />
            </run>
        </groups>
        <classes>
            <!-- 可以多个 -->
            <class name="org.airwallex.test.EURToAUDTest"/>
            <class name="org.airwallex.test.EURToCNYTest"/>
            <class name="org.airwallex.test.EURToHKDTest"/>
            <class name="org.airwallex.test.SGDToAUDTest"/>
            <class name="org.airwallex.test.SGDToCNYTest"/>
            <class name="org.airwallex.test.SGDToHKDTest"/>
            <class name="org.airwallex.test.USDToAUDTest"/>
            <class name="org.airwallex.test.USDToCNYTest"/>
            <class name="org.airwallex.test.USDToHKDTest"/>
        </classes>
    </test>

</suite>